{
  "name": "vike-photon",
  "version": "0.1.7",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./api": "./dist/api.js",
    "./config": "./dist/config.js",
    "./plugin": "./dist/plugin.js",
    "./photon-types": {
      "types": "./photon.d.ts"
    },
    "./universal-middlewares": {
      "types": "./dist/universal.d.ts",
      "development": {
        "types": "./dist/universal.dev.d.ts",
        "import": "./dist/universal.dev.js",
        "default": "./dist/universal.dev.js"
      },
      "worker": {
        "types": "./dist/universal.edge.d.ts",
        "import": "./dist/universal.edge.js",
        "default": "./dist/universal.edge.js"
      },
      "import": "./dist/universal.js",
      "default": "./dist/universal.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup && pnpm install --ignore-scripts",
    "release": "release-me patch",
    "test:types": "tsc --noEmit",
    "release:minor": "release-me minor",
    "release:major": "release-me major",
    "release:commit": "release-me commit"
  },
  "dependencies": {
    "@brillout/picocolors": "^1.0.28",
    "@brillout/vite-plugin-server-entry": "^0.7.15",
    "@photonjs/core": "0.0.*",
    "@photonjs/runtime": "0.0.*",
    "@universal-middleware/compress": "^0.2.31",
    "@universal-middleware/core": "^0.4.10",
    "@universal-middleware/sirv": "^0.1.21",
    "esbuild": "^0.25.9",
    "local-pkg": "^1.1.2"
  },
  "peerDependencies": {
    "@photonjs/core": "0.0.*",
    "@photonjs/runtime": "0.0.*",
    "@photonjs/cloudflare": ">=0.0.9",
    "vike": ">=0.4.238",
    "vite": ">=7.1",
    "vite-plugin-vercel": "^10.0.0-beta.4"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "@photonjs/cloudflare": {
      "optional": true
    },
    "vite-plugin-vercel": {
      "optional": true
    }
  },
  "devDependencies": {
    "@brillout/release-me": "^0.4.8",
    "@elysiajs/node": "^1.4.0",
    "@hattip/adapter-node": "^0.0.49",
    "@hono/node-server": "^1.19.3",
    "@photonjs/cloudflare": "0.0.*",
    "@types/bun": "^1.2.22",
    "@types/deno": "^2.3.0",
    "@types/node": "^20.19.16",
    "elysia": "^1.4.5",
    "fastify": "^5.6.0",
    "h3": "^1.15.4",
    "hono": "^4.9.7",
    "magic-string": "^0.30.19",
    "rollup": "4.38.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vike": "^0.4.240",
    "vite": "^7.1.5",
    "vite-plugin-vercel": "^10.0.0-beta.4"
  },
  "files": [
    "dist/",
    "photon.d.ts"
  ],
  "repository": "github:vikejs/vike-photon",
  "license": "MIT"
}
